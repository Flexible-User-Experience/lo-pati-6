{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "ext-calendar": "*",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "ext-filter": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-zip": "*",
        "ext-zlib": "*",
        "beberlei/doctrineextensions": "^1.5",
        "cocur/slugify": "^4.5",
        "connectholland/cookie-consent-bundle": "dev-master",
        "doctrine/dbal": "^3.8",
        "doctrine/doctrine-bundle": "^2.12",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.2",
        "egulias/email-validator": "^4.0",
        "excelwebzone/recaptcha-bundle": "^1.5",
        "fortawesome/font-awesome": "^6.6",
        "knplabs/knp-menu-bundle": "^3.4",
        "knplabs/knp-paginator-bundle": "^6.4",
        "league/commonmark": "^2.5",
        "league/csv": "^9.16",
        "league/html-to-markdown": "^5.1",
        "leapt/froala-editor-bundle": "^1.7",
        "liip/imagine-bundle": "^2.13",
        "meilisearch/search-bundle": "^0.15.4",
        "nyholm/psr7": "^1.8",
        "phpdocumentor/reflection-docblock": "^5.4",
        "phpstan/phpdoc-parser": "^1.29",
        "presta/sitemap-bundle": "^4.1",
        "sendgrid/sendgrid": "^8.1",
        "sonata-project/admin-bundle": "^4.31",
        "sonata-project/doctrine-orm-admin-bundle": "^4.17",
        "stof/doctrine-extensions-bundle": "^1.12",
        "symfony/asset": "6.4.*",
        "symfony/asset-mapper": "6.4.*",
        "symfony/console": "6.4.*",
        "symfony/doctrine-messenger": "6.4.*",
        "symfony/dotenv": "6.4.*",
        "symfony/event-dispatcher": "6.4.*",
        "symfony/expression-language": "6.4.*",
        "symfony/flex": "^2.4",
        "symfony/form": "6.4.*",
        "symfony/framework-bundle": "6.4.*",
        "symfony/http-client": "6.4.*",
        "symfony/intl": "6.4.*",
        "symfony/mailer": "6.4.*",
        "symfony/mime": "6.4.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/notifier": "6.4.*",
        "symfony/process": "6.4.*",
        "symfony/property-access": "6.4.*",
        "symfony/property-info": "6.4.*",
        "symfony/runtime": "6.4.*",
        "symfony/scheduler": "6.4.*",
        "symfony/security-bundle": "6.4.*",
        "symfony/sendgrid-mailer": "6.4.*",
        "symfony/serializer": "6.4.*",
        "symfony/stimulus-bundle": "^2.18",
        "symfony/string": "6.4.*",
        "symfony/translation": "6.4.*",
        "symfony/twig-bundle": "6.4.*",
        "symfony/ux-live-component": "^2.19",
        "symfony/ux-turbo": "^2.18",
        "symfony/ux-twig-component": "^2.18",
        "symfony/validator": "6.4.*",
        "symfony/web-link": "6.4.*",
        "symfony/yaml": "6.4.*",
        "symfonycasts/sass-bundle": "^0.7",
        "twbs/bootstrap": "^5.3",
        "twig/cssinliner-extra": "^3.10",
        "twig/extra-bundle": "^3.10",
        "twig/inky-extra": "^3.10",
        "twig/markdown-extra": "^3.10",
        "twig/twig": "^3.14",
        "vich/uploader-bundle": "^2.4"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:davidromani/harborn-cookie-consent-bundle.git"
        }
    ],
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "froala:install": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd",
            "sass:build": "symfony-cmd",
            "asset-map:compile": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.4.*"
        }
    },
    "require-dev": {
        "hautelook/alice-bundle": "^2.9",
        "phpunit/phpunit": "^10.5",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "6.4.*",
        "symfony/css-selector": "6.4.*",
        "symfony/debug-bundle": "6.4.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^7.1",
        "symfony/stopwatch": "6.4.*",
        "symfony/web-profiler-bundle": "6.4.*"
    }
}
